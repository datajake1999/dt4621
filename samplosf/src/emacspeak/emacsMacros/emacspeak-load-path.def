(defvar emacspeak-dir
  (expand-file-name "<emacspeak-dir>")
  "Directory where emacspeak is installed. ")
(or (member emacspeak-dir load-path )
(setq load-path
      (cons emacspeak-dir 
                              load-path )))

(provide 'emacspeak-load-path)
