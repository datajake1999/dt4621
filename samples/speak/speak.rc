SPEAK_MENU MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New",                        IDM_FILE_NEW
        MENUITEM "&Open...",                    IDM_FILE_OPEN
        MENUITEM "&Save",                       IDM_SAVE
        MENUITEM "S&ave as...",                 IDM_SAVEAS
        MENUITEM "&Close",                      IDM_FILE_CLOSE
        MENUITEM SEPARATOR
#if (!defined DEMO) || (defined SALESFORCE) 
        MENUITEM "&Load User Dictionary...",    IDM_LOAD_DIC
        MENUITEM "&Unload User Dictionary",     IDM_UNLOAD_DIC
        POPUP "&Convert to Wave File"
        BEGIN
            MENUITEM "MONO 11.025kHz, 16-Bit",      IDM_WAVE1116
            MENUITEM "MONO 11.025kHz, 8-Bit ",      IDM_WAVE1108
            MENUITEM "MONO 8kHz, µLaw",             IDM_MULAW
        END
#else
        MENUITEM "&Load User Dictionary...",    IDM_LOAD_DIC, GRAYED
        MENUITEM "&Unload User Dictionary",     IDM_UNLOAD_DIC, GRAYED
        POPUP "&Convert to Wave File"
        BEGIN
            MENUITEM "MONO 11.025kHz, 16-Bit",      IDM_WAVE1116, GRAYED
            MENUITEM "MONO 11.025kHz, 8-Bit ",      IDM_WAVE1108, GRAYED
            MENUITEM "MONO 8kHz, µLaw",             IDM_MULAW, GRAYED
        END
#endif
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       IDM_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t",                        ID_EDIT_CUT
        MENUITEM "&Copy",                       ID_EDIT_COPY
        MENUITEM "&Paste",                      ID_EDIT_PASTE
        MENUITEM "De&lete",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "Select &All",                 ID_EDIT_SELECT_ALL
        MENUITEM SEPARATOR
        MENUITEM "&Find...",                    ID_FIND
        MENUITEM SEPARATOR
        MENUITEM "English",                     IDM_ENGLISH, GRAYED
        MENUITEM "Spanish",                     IDM_SPANISH, GRAYED
        MENUITEM "German",                      IDM_GERMAN, GRAYED
        MENUITEM "Latin American",              IDM_LATIN, GRAYED
        MENUITEM "English UK",					IDM_ENGLISH_UK, GRAYED
        MENUITEM "French",                      IDM_FRENCH, GRAYED
    END
    POPUP "&Help"
    BEGIN
#ifndef DEMO
        MENUITEM "&Help on DECtalk ",           IDM_HELP
#endif
        MENUITEM "&About Speak",                IDM_ABOUT
    END
END


