/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

WINDIC_MENU MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open...",                    IDM_FILE_OPEN
        MENUITEM "Close",                       IDM_FILE_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "&Compile Dictionary",         IDM_COMPILE_DICTIONARY
        MENUITEM SEPARATOR
        MENUITEM "&Save...",                    IDM_SAVE
        MENUITEM "S&ave as...",                 IDM_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       IDM_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t\tCtrl+X",                ID_EDIT_CUT
        MENUITEM "&Copy\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM "De&lete\tDel",                ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "Select &All",                 ID_EDIT_SELECT_ALL
        MENUITEM SEPARATOR
        MENUITEM "&Find...",                    ID_FIND
    END
    MENUITEM "&Translate",                  ID_XLATE
    POPUP "&Language"
    BEGIN
        MENUITEM "&English US",                 ID_LANGUAGE_ENGLISH, GRAYED
        MENUITEM "&Spanish",                    ID_LANGUAGE_SPANISH, GRAYED
        MENUITEM "&German",                     ID_LANGUAGE_GERMAN, GRAYED
        MENUITEM "&Latin American",             ID_LANGUAGE_LATIN_AMERICAN
        , GRAYED
        MENUITEM "English U&K",                 ID_LANGUAGE_BRITISH, GRAYED
        MENUITEM "&French",						ID_LANGUAGE_FRENCH, GRAYED
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Help on DECtalk ",           IDM_HELP
        MENUITEM "&About windic",               IDM_ABOUT
    END
END


